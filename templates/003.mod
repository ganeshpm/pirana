$PROBLEM 2-comp iv, linear elim

$INPUT ID TIME DV AMT CMT MDV EVID

$DATA nm_001.csv ; IGNORE=

$SUBROUTINES ADVAN3 TRANS4

$PK
CL = THETA(1) * EXP(ETA(1))
V1 = THETA(2) * EXP(ETA(2))
Q  = THETA(3) * EXP(ETA(3))
V2 = THETA(4)
S1 = V1

$ERROR
IPRED = F
    Y = IPRED + EPS(1) 
    W = 1
  RES = DV-IPRED
IWRES = RES/W

$THETA
(1, 5, 50) ; CL
(1, 50, 150) ; V
(1, 5, 50) ; Q

$OMEGA BLOCK(2)
(0.2) ; IIV CL
(0.1) ; CL~V
(0.2) ; IIV V

$SIGMA
(0.2) ; Additive error

$EST METHOD=1 INTER MAXEVAL=2000 NOABORT SIG=3 PRINT=1 POSTHOC
$COV

; Xpose
$TABLE ID TIME DV MDV EVID IPRED IWRES FILE=sdtab003
$TABLE CL V KA Q V2 FIRSTONLY FILE=patab003

