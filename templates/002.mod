$PROBLEM 1-comp oral, linear elim

$INPUT ID TIME DV AMT CMT MDV EVID

$DATA nm_001.csv IGNORE=@

$SUBROUTINES ADVAN2 TRANS2

$PK
CL = THETA(1) * EXP(ETA(1))
V  = THETA(2) * EXP(ETA(2))
KA = THETA(3) * EXP(ETA(3))
S1 = V

$ERROR
IPRED = F
    Y = IPRED + EPS(1) 
    W = 1
  RES = DV-IPRED
IWRES = RES/W

$THETA
(1, 5, 50) ; CL
(1, 50, 150) ; V
(0.1, 0.5, 2) ; KA

$OMEGA BLOCK(2)
(0.2) ; IIV CL
(0.1) ; CL~V
(0.2) ; IIV V
$OMEGA 
(0.2) ; KA

$SIGMA
(0.2) ; Additive error

$EST METHOD=1 INTER MAXEVAL=2000 NOABORT SIG=3 PRINT=1 POSTHOC
$COV

; Xpose
$TABLE ID TIME DV MDV EVID IPRED IWRES FILE=sdtab002
$TABLE CL V KA FIRSTONLY FILE=patab001

